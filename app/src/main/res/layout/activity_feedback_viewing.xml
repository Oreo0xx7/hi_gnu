<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FeedbackViewing">

    <ImageButton
        android:id="@+id/img_support"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:background="#FFFFFF"
        android:tint="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/tv_lo_support"
        app:layout_constraintEnd_toStartOf="@+id/tv_lo_support"
        app:layout_constraintStart_toStartOf="@+id/tv_fd"
        app:layout_constraintTop_toTopOf="@+id/tv_lo_support"
        app:srcCompat="@android:drawable/stat_sys_headset" />
    <TextView
        android:id="@+id/tv_lo_support"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Support"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_fd"
        app:layout_constraintEnd_toEndOf="@+id/tv_fd"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/tv_fd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Feedback Board"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/et_fd_title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider"
        android:layout_width="40dp"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_fd" />


</android.support.constraint.ConstraintLayout>